<div class="app">
  <header>
    <div class="service_name">
      <h1 gorilla-click="reloadClick"><img src="assets/images/main_logo.png"><%= title %></h1>
      <h4><%= subTitle %></h4>
    </div>
    <div class="search_bar">
      <input id="search_box" type="text" placeholder='<%= searchboxPlaceholder %>' gorilla-keypress="handleKeypress">
      <button type="submit" id="search_button" gorilla-click="handleSearchClick">GO</button>
    </div>
  </header>
  <%= contents %>
  <div id="get_more">
    <button id="get_more_button" class="">More</button>
  </div>
  <button id="to_upward" class="" gorilla-click="handleUpwardClick"><img src="assets/images/to_upward.png"></button>
  <footer>
    <span>Any questions ? &nbsp;&nbsp;<%= contact %></span>
  </footer>
</div>