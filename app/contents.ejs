<div id="main_content">
  <div class="content_header">
    <p class="content_result"><span>"<%= keyword %>"</span>&nbsp;&nbsp;검색결과</p>
    <div class="select_view">
      <button ><img src="/assets/images/list_button.png" class="list_view_button selected"></button>
      <button ><img src="/assets/images/card_button.png" class="card_view_button"></button>
    </div>
  </div>
  <div class="content_area">
    <% contentData.forEach(function(item){ %>
      <div class="content_container">
        <div class="container_left">
          <a href="<%= item.link %>">
            <img src="<%= item.image %>" onerror="event.currentTarget.src='/assets/images/replace_img.jpg'">
          </a>
        </div>
        <div class="container_right">
          <h1 class="content_title"><%= item.title %></h1>
          <p class="content_subtitle"><%= item.subTitle %></p>
          <div class="content_main">
            <p class="content_author"><%= item.author %> | <%= item.publisher %> | <%= item.pubdate %> | <a href="<%= item.link %>" class="content_url"><%= item.link %></a></p>
            <p class="content_description"><%= item.description %></p>
            <p class="content_price">
              <span>구매</span>
              <span class="content_discount_price"><%= item.discount %></span>
              <span class="content_sales_price"><%= item.price %></span>
            </p>
          </div>
        </div>
      </div>
    <% }); %>
  </div>
</div>